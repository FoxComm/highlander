<table><thead>
<tr>
    <th>Field</th>
    <th>Type</th>
    <th>Required</th>
    <th>Example</th>
    <th>Description</th>
</tr>
</thead><tbody>
{{#each documentables}}
    <tr id='{{id}}'>
        <td><code><b>{{name}}</b></code></td>
        <td><code>{{{type type}}}</code>
        <td>{{#unless optional}}<div data-icon="ei-check"></div>{{/unless}}</td>
        <td>{{{markdown defaultValue}}}</td>
        <td>{{{markdown comments}}}</td>
    </tr>
{{/each}}
</tbody></table>
