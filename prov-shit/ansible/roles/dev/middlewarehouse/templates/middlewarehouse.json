{
  "id": "/middlewarehouse",
  "cmd": null,
  "cpus": 1,
  "mem": 64,
  "disk": 0,
  "instances": 1,
  "container": {
    "type": "DOCKER",
    "volumes": [],
    "docker": {
      "image": "docker.stage.foxcommerce.com:5000/middlewarehouse",
      "network": "HOST",
      "privileged": false,
      "parameters": [],
      "forcePullImage": false
    }
  },
  "env": {
    "DB_SSLMODE": "disable",
    "SCHEMA_REGISTRY_URL": "http://schema-registry.service.consul:8081",
    "DB_NAME": "middlewarehouse_development",
    "PHOENIX_URL": "http://phoenix.service.consul:9090",
    "ZOOKEEPER_URL": "zookeeper.service.consul:2181",
    "LOG_LEVEL": "info",
    "DB_HOST": "db.service.consul",
    "KAFKA_BROKER": "kafka.service.consul:9092",
    "DB_USER": "middlewarehouse"
  },
  "healthChecks": [
    {
      "path": "/v1/public/ping",
      "protocol": "HTTP",
      "gracePeriodSeconds": 300,
      "intervalSeconds": 60,
      "timeoutSeconds": 20,
      "maxConsecutiveFailures": 3,
      "ignoreHttp1xx": false,
      "port": 9292
    }
  ],
  "portDefinitions": [
    {
      "port": 10000,
      "protocol": "tcp",
      "name": "9292",
      "labels": {}
    }
  ]
}