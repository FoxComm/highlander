{
  "id": "/middlewarehouse",
  "cmd": null,
  "cpus": 0.25,
  "mem": 64,
  "disk": 0,
  "instances": 1,
  "container": {
    "type": "DOCKER",
    "volumes": [],
    "docker": {
      "image": "{{docker_registry}}:5000/middlewarehouse:{{docker_branch}}",
      "network": "HOST",
      "privileged": false,
      "parameters": [],
      "forcePullImage": true
    }
  },
  "env": {
    "DB_SSLMODE": "disable",
    "SCHEMA_REGISTRY_URL": "{{schema_server}}",
    "DB_NAME": "{{middlewarehouse_db_name}}",
    "PHOENIX_URL": "http://{{phoenix_server}}",
    "ZOOKEEPER_URL": "{{zookeeper_server}}",
    "LOG_LEVEL": "info",
    "DB_HOST": "{{docker_db_host}}",
    "KAFKA_BROKER": "{{kafka_server}}",
    "DB_USER": "middlewarehouse"
  },
  "healthChecks": [
    {
      "path": "/v1/public/ping",
      "protocol": "HTTP",
      "gracePeriodSeconds": 300,
      "intervalSeconds": 60,
      "timeoutSeconds": 20,
      "maxConsecutiveFailures": 3,
      "ignoreHttp1xx": false,
      "port": 9292
    }
  ],
  "portDefinitions": [
    {
      "port": 10000,
      "protocol": "tcp",
      "name": "9292",
      "labels": {}
    }
  ]
}
