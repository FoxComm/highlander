{
  "id": "/ashes",
  "cmd": null,
  "cpus": 1,
  "mem": 1024,
  "disk": 0,
  "instances": 1,
  "container": {
    "type": "DOCKER",
    "volumes": [],
    "docker": {
      "image": "docker.topdrawer.foxcommerce.com:5000/ashes",
      "network": "HOST",
      "privileged": false,
      "parameters": [],
      "forcePullImage": true
    }
  },
  "env": {
    "NODE_TLS_REJECT_UNAUTHORIZED": "0",
    "API_URL": "http://balancer.service.consul",
    "NODE_ENV": "production",
    "PORT": "4040",
    "PHOENIX_PUBLIC_KEY": "/ashes/public_key.pem",
    "GA_TRACKING_ID": "UA-79356556-1"
  },
  "healthChecks": [
    {
      "path": "/login",
      "protocol": "HTTP",
      "gracePeriodSeconds": 300,
      "intervalSeconds": 60,
      "timeoutSeconds": 20,
      "maxConsecutiveFailures": 3,
      "ignoreHttp1xx": false,
      "port": 4040
    }
  ],
  "portDefinitions": [
    {
      "port": 4040,
      "protocol": "tcp",
      "name": "4040",
      "labels": {}
    }
  ]
}