{
  "id": "/marketplace-ui",
  "cpus": 1,
  "mem": 512,
  "instances": 1,
  "container": {
    "type": "DOCKER",
    "volumes": [],
    "docker": {
      "image": "{{docker_registry}}/marketplace-ui",
      "network": "HOST",
      "privileged": false,
      "parameters": [],
