{
  "id": "/suggester",
  "cmd": null,
  "cpus": 0.25,
  "mem": 64,
  "disk": 0,
  "instances": 1,
  "container": {
    "type": "DOCKER",
    "volumes": [
      {
        "containerPath": "{{docker_logs_dir}}",
        "hostPath": "{{docker_logs_host_dir}}",
        "mode": "RW"
      }
    ],
    "docker": {
      "image": "{{docker_registry}}:5000/suggester:{{docker_tags.suggester}}",
      "network": "HOST",
      "portMappings": null,
      "privileged": false,
      "parameters": [],
      "forcePullImage": true
    }
  },
  "env": {
    "ANTHILL_HOST": "{{anthill_host}}"
  },
  "healthChecks": [
    {
      "protocol": "COMMAND",
      "command": {
        "value": "ps -ef | grep -v grep | grep suggester"
      },
      "gracePeriodSeconds": 300,
      "intervalSeconds": 30,
      "timeoutSeconds": 20,
      "maxConsecutiveFailures": 3,
      "ignoreHttp1xx": false
    }
  ],
  "labels": {
    "LAYER": "consumer",
    "LANG": "go",
    "TAG": "master"
  },
  "portDefinitions": [
    {
      "port": 10030,
      "protocol": "tcp",
      "labels": {}
    }
  ]
}