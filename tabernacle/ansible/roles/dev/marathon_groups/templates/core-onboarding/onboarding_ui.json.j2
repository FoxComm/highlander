{
    "id": "onboarding-ui",
    "cmd": null,
    "cpus": 1,
    "mem": 1024,
    "disk": 0,
    "instances": 1,
    "labels": {
        "LANG": "js",
        "subdomain": "onboarding-ui",
        "TAG": "{{docker_tags.onboarding_ui}}"
    },
    "container": {
        "type": "DOCKER",
        "volumes": [{
            "containerPath": "{{public_keys_dest_dir}}",
            "hostPath": "{{public_keys_dest_dir}}",
            "mode": "RO"
        }],
        "docker": {
            "image": "{{docker_registry}}:5000/onboarding-ui:{{docker_tags.onboarding_ui}}",
            "network": "HOST",
            "portMappings": [],
            "privileged": false,
            "parameters": [],
            "forcePullImage": true
        }
    },
    "env": {
        "API_URL": "https://{{storefront_server_name}}/api/v1/onboarding",
        "NODE_ENV": "production",
        "ASHES_URL": "https://{{storefront_server_name}}/admin",
        "GA_TRACKING_ID": "{{ga_tracking_id}}",
        "STRIPE_PUBLISHABLE_KEY": "{{stripe_publishable_key}}"
    },
    "healthChecks": [{
        "gracePeriodSeconds": 300,
        "intervalSeconds": 30,
        "timeoutSeconds": 20,
        "maxConsecutiveFailures": 3,
        "portIndex": 0,
        "path": "/",
        "protocol": "HTTP",
        "ignoreHttp1xx": false
    }],
    "portDefinitions": [{
        "port": 10003,
        "protocol": "tcp",
        "name": "default",
        "labels": {}
    }]
}
