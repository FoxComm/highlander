{
  "name": "ashes",
  "version": "0.0.2",
  "description": "FoxCommerce Admin UI",
  "private": true,
  "main": "server/index.js",
  "engines": {
    "node": ">=4.2.0"
  },
  "os": [
    "!win32"
  ],
  "scripts": {
    "test": "make test",
    "start": "node --harmony boot.js",
    "stop": "kill -USR2 $(ps aux | grep '[m]-ashes' | awk '{print $2}')",
    "build": "./node_modules/.bin/gulp",
    "dev": "./node_modules/.bin/gulp dev",
    "flow": "./node_modules/.bin/flow check",
    "lint": "./node_modules/.bin/gulp lint"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/FoxComm/Ashes.git"
  },
  "keywords": [
    "FoxCommerce",
    "Ecommerce",
    "CMS"
  ],
  "author": "FoxCommerce Engineering",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/FoxComm/Ashes/issues"
  },
  "contributors": "https://github.com/FoxComm/Ashes/graphs/contributors",
  "homepage": "https://github.com/FoxComm/Ashes#readme",
  "eslintConfig": {
    "env": {
      "node": true,
      "browser": true,
      "es6": true,
      "mocha": true
    },
    "plugins": [
      "react"
    ],
    "ecmaFeatures": {
      "jsx": true,
      "modules": true
    },
    "globals": {
      "expect": false
    },
    "rules": {
      "max-len": [
        1,
        120,
        2,
        {
          "ignoreComments": true,
          "ignoreUrls": true,
          "ignorePattern": "^.+=\\s*require\\s*\\("
        }
      ],
      "quotes": [
        1,
        "single"
      ],
      "curly": [
        2,
        "multi-line"
      ],
      "no-process-exit": 0,
      "no-underscore-dangle": 0,
      "no-multi-spaces": [
        1,
        {
          "exceptions": {
            "VariableDeclarator": true
          }
        }
      ],
      "react/display-name": 0,
      "react/jsx-uses-react": 1,
      "react/jsx-uses-vars": 1,
      "react/no-did-mount-set-state": 1,
      "react/no-did-update-set-state": 1,
      "react/no-multi-comp": [
        1,
        {
          "ignoreStateless": true
        }
      ],
      "react/prop-types": 1,
      "react/react-in-jsx-scope": 1,
      "react/self-closing-comp": 1,
      "react/wrap-multilines": 1
    }
  },
  "dependencies": {
    "babel-polyfill": "^6.5.0",
    "browserHistory": "file:src/opt/browserHistory",
    "chance": "^0.7.6",
    "classnames": "^2.1.3",
    "cli-color": "^1.0.0",
    "co": "^4.5.4",
    "co-body": "^4.0.0",
    "core-decorators": "^0.8.0",
    "currency-symbol-map": "^2.1.0",
    "draft-js": "^0.7.0",
    "draft-js-export-html": "^0.2.2",
    "draft-js-import-html": "^0.1.10",
    "event-source-polyfill": "0.0.6",
    "fleck": "^0.5.1",
    "history": "^2.0.2",
    "htmlescape": "^1.1.0",
    "imagemin-pngquant": "^4.2.0",
    "koa": "^1.0.0",
    "koa-body": "^1.2.1",
    "koa-favicon": "^1.2.0",
    "koa-json": "^1.1.1",
    "koa-proxy": "^0.4.1",
    "koa-router": "^5.1.2",
    "koa-static": "^1.4.9",
    "less-plugin-autoprefix": "^1.4.2",
    "less-plugin-group-css-media-queries": "^1.0.1",
    "less-plugin-npm-import": "^2.1.0",
    "lodash": "^3.10.1",
    "moment": "^2.10.3",
    "normalize.css": "^3.0.3",
    "react": "^15.0.2",
    "react-addons-create-fragment": "^15.0.2",
    "react-addons-css-transition-group": "^15.0.2",
    "react-dnd": "^2.1.4",
    "react-dnd-html5-backend": "^2.1.2",
    "react-dom": "^15.0.2",
    "react-input-mask": "anru/react-input-mask#a7b8dc04debe9e8dfe7eee3ad31facb79b7c1826",
    "react-motion": "^0.4.4",
    "react-redux": "^4.4.0",
    "react-router": "^2.3.0",
    "react-router-redux": "^4.0.2",
    "reduce-reducers": "^0.1.1",
    "redux": "^3.0.2",
    "redux-act": "^0.4.2",
    "redux-logger": "^2.0.3",
    "redux-thunk": "^1.0.0",
    "request": "^2.58.0",
    "reselect": "^2.0.0",
    "sprout-data": "^0.2.3",
    "superagent": "^2.0.0",
    "thunkify-wrap": "^1.0.4",
    "use-named-routes": "^0.3.0",
    "wings": "github:foxcomm/wings#v1.1.0",
    "zipcodes-regex": "^1.0.0"
  },
  "devDependencies": {
    "babel-core": "^6.6.5",
    "babel-eslint": "^4.1.8",
    "babel-plugin-react-transform": "^2.0.0",
    "babel-plugin-transform-class-properties": "~6.5.2",
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "babel-plugin-transform-runtime": "~6.5.0",
    "babel-plugin-transform-styles": "file:src/opt/babel-plugin-transform-styles",
    "babel-polyfill": "~6.5.0",
    "babel-preset-es2015": "~6.5.0",
    "babel-preset-react": "~6.5.0",
    "babel-preset-stage-0": "~6.5.0",
    "babel-register": "^6.9.0",
    "babel-runtime": "~6.5.0",
    "babelify": "^7.2.0",
    "browserify": "^13.0.0",
    "canvas": "^1.4.0",
    "chai": "^3.0.0",
    "co-mocha": "^1.1.2",
    "css-modulesify": "github:anru/css-modulesify#update-me-to-0-23-version-later",
    "cssnano": "^3.7.3",
    "envify": "^3.4.0",
    "eslint": "^1.10.1",
    "eslint-plugin-react": "^3.10.0",
    "flow-bin": "0.23.0",
    "gulp": "^3.9.0",
    "gulp-concat": "^2.6.0",
    "gulp-cssnano": "^2.1.2",
    "gulp-eslint": "^1.0.0",
    "gulp-if": "^1.2.5",
    "gulp-imagemin": "^2.3.0",
    "gulp-less": "^3.0.3",
    "gulp-load-plugins": "^1.0.0-rc.1",
    "gulp-mocha": "^2.1.2",
    "gulp-plumber": "^1.0.1",
    "gulp-postcss": "^6.1.0",
    "gulp-rename": "^1.2.2",
    "gulp-sourcemaps": "^1.5.2",
    "gulp-spawn-mocha": "^2.2.2",
    "gulp-uglify": "^1.5.4",
    "gulp-util": "^3.0.6",
    "ignore-styles": "^1.2.0",
    "jsdom": "^9.4.1",
    "jsonwebtoken": "^5.5.4",
    "koa-logger": "^1.2.2",
    "localStorage": "^1.0.3",
    "lost": "^6.7.2",
    "merge-stream": "^1.0.0",
    "mocha": "^2.3.0",
    "nock": "^8.0.0",
    "node-notifier": "^4.2.3",
    "postcss-css-variables": "^0.5.1",
    "postcss-cssnext": "^2.5.1",
    "postcss-import": "^8.0.2",
    "postcss-modules-extract-imports": "^1.0.0",
    "postcss-modules-local-by-default": "^1.0.1",
    "postcss-modules-scope": "^1.0.0",
    "postcss-modules-values": "^1.1.2",
    "query-string": "^4.2.2",
    "react-addons-test-utils": "^0.14.0",
    "react-shallow-testutils": "^0.6.0",
    "rewire": "^2.3.4",
    "rimraf": "^2.4.2",
    "run-sequence": "^1.2.2",
    "sinon": "^1.16.1",
    "strip-ansi": "^3.0.0",
    "through2": "^2.0.0",
    "uglify-js": "^2.7.0",
    "unexpected": "^10.0.2",
    "unexpected-react-shallow": "^0.7.0",
    "vinyl-buffer": "^1.0.0",
    "vinyl-source-stream": "^1.1.0",
    "watchify": "^3.3.1",
    "xmlhttprequest": "^1.7.0"
  }
}
