{
  "name": "ashes",
  "version": "0.0.1",
  "description": "FoxCommerce CMS",
  "private": true,
  "main": "server/index.js",
  "scripts": {
    "test": "make test",
    "start": "iojs --harmony boot.js",
    "restart": "kill -USR2 $(ps aux | grep '[m]-ashes' | awk '{print $2}')",
    "build": "./node_modules/.bin/gulp",
    "dev": "./node_modules/.bin/gulp dev"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/FoxComm/Ashes.git"
  },
  "keywords": [
    "FoxCommerce",
    "Ecommerce",
    "CMS"
  ],
  "author": "Jonathan 'Tivs' Rainey",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/FoxComm/Ashes/issues"
  },
  "homepage": "https://github.com/FoxComm/Ashes#readme",
  "eslintConfig": {
    "env": {
      "node": true,
      "browser": true,
      "es6": true
    },
    "plugins": [
      "react"
    ],
    "ecmaFeatures": {
      "jsx": true,
      "modules": true
    },
    "rules": {
      "quotes": "single",
      "curly": [
        2,
        "multi-line"
      ],
      "no-process-exit": false,
      "no-underscore-dangle": false,
      "no-multi-spaces": [
        1,
        {
          "exceptions": {
            "VariableDeclarator": true
          }
        }
      ],
      "react/display-name": 0,
      "react/jsx-uses-react": 1,
      "react/jsx-uses-vars": 1,
      "react/no-did-mount-set-state": 1,
      "react/no-did-update-set-state": 1,
      "react/no-multi-comp": 1,
      "react/prop-types": 1,
      "react/react-in-jsx-scope": 1,
      "react/self-closing-comp": 1,
      "react/wrap-multilines": 1
    }
  },
  "dependencies": {
    "babelify": "^6.1.0",
    "browserify": "^10.2.4",
    "cli-color": "^1.0.0",
    "co": "^4.5.4",
    "gulp": "^3.9.0",
    "gulp-concat": "^2.5.2",
    "gulp-if": "^1.2.5",
    "gulp-imagemin": "^2.2.1",
    "gulp-less": "^3.0.3",
    "gulp-load-plugins": "^1.0.0-rc.1",
    "gulp-rename": "^1.2.2",
    "gulp-sourcemaps": "^1.5.2",
    "gulp-uglify": "^1.2.0",
    "htmlescape": "^1.1.0",
    "imagemin-pngquant": "^4.1.0",
    "koa": "^0.21.0",
    "koa-favicon": "^1.2.0",
    "koa-json": "^1.1.1",
    "koa-router": "^5.1.2",
    "koa-static": "^1.4.9",
    "less-plugin-autoprefix": "^1.4.2",
    "less-plugin-clean-css": "^1.5.0",
    "less-plugin-group-css-media-queries": "^1.0.1",
    "less-plugin-npm-import": "^2.1.0",
    "merge-stream": "^0.1.7",
    "moment": "^2.10.3",
    "normalize.css": "^3.0.3",
    "react": "^0.13.3",
    "react-router": "^0.13.3",
    "reactify": "^1.1.1",
    "request": "^2.57.0",
    "thunkify-wrap": "^1.0.4",
    "underscore": "^1.8.3",
    "vinyl-buffer": "^1.0.0",
    "vinyl-source-stream": "^1.1.0"
  },
  "devDependencies": {
    "chai": "^3.0.0",
    "co-mocha": "^1.1.0",
    "eslint": "^0.22.1",
    "eslint-plugin-react": "^2.5.0",
    "gulp-eslint": "^0.13.2",
    "koa-logger": "^1.2.2",
    "mocha": "^2.2.4"
  }
}
