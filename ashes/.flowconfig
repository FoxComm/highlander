[ignore]
.*/node_modules/react/.*
.*/node_modules/jss/.*
.*/node_modules/react-motion/src/*
.*/node_modules/react-motion/lib/Motion.js.flow
.*/node_modules/react-motion/lib/Types.js.flow
.*/node_modules/nock/*
.*/node_modules/findup/.*
.*/node_modules/stylelint/.*

[include]

[libs]
src/interfaces/
src/interfaces/paragons/

[options]
unsafe.enable_getters_and_setters=true
esproposal.decorators=ignore
module.name_mapper='.+css$' -> 'CSSModule'
module.system.node.resolve_dirname=node_modules
module.system.node.resolve_dirname=src
suppress_comment=\\(.\\|\n\\)*\\$FlowFixMe

[version]
>=0.45.0
